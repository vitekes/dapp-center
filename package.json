{
  "name": "dapp-center",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prune": "ts-prune --ignore \"\\.test\\.|\\.stories\\.|types\\.ts$\"",
    "depcheck": "depcheck --ignores=\"eslint,ts-node,tailwindcss,postcss\"",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "deploy:local": "hardhat deploy --network localhost && ts-node scripts/genFrontendEnv.ts localhost",
    "deploy:sepolia": "hardhat deploy --network sepolia && ts-node scripts/genFrontendEnv.ts sepolia",
    "deploy": "hardhat deploy --network $NETWORK && ts-node scripts/genFrontendEnv.ts $NETWORK",
    "env:gen": "ts-node scripts/genFrontendEnv.ts localhost"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.0.1",
    "@lighthouse-web3/sdk": "^0.3.7",
    "@rainbow-me/rainbowkit": "^2.2.5",
    "@tanstack/react-query": "^5",
    "blurhash": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.14.3",
    "ipfs-http-client": "^60.0.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "react-hot-toast": "^2.5.2",
    "siwe": "^3.0.0",
    "tailwind-merge": "^3.3.0",
    "viem": "^2.30.1",
    "wagmi": "^2.15.4",
    "zod": "^3.25.36",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.11",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.11",
    "@openzeppelin/contracts": "^5.3.0",
    "@tailwindcss/postcss": "^4",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "chai": "^4.2.0",
    "depcheck": "^1.4.7",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "hardhat": "^2.24.1",
    "hardhat-deploy": "^1.0.2",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.4",
    "typechain": "^8.3.2",
    "typescript": "^5"
  },
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}
